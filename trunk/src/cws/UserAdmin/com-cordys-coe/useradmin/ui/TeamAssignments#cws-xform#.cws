function UITeamUsersModel_OnResponse(eventObject)
{
    if (eventObject.httpStatusCode != 500)
    {
        assignmentsChanged = false;
        checkEnableDisable();
        
        uiAssignments.length = 0;
        listTeamUsers.removeOptions(listTeamUsers.getAllOptions(), false);
        listTeamUsers.removeOptions(listTeamUsers.getAllOptions(), false);
        var response = eventObject.response;
        var tupleNode = cordys.selectXMLNode(response, ".//*[local-name()='tuple']/*[local-name()='new']");
        if (tupleNode == null)
        {
           // regular get response instead of synchronize response
           tupleNode = cordys.selectXMLNode(response, ".//*[local-name()='tuple']/*[local-name()='old']");
        }
        var uiTeamUserNodes = cordys.selectXMLNodes(tupleNode, ".//*[local-name()='Assignments']/*[local-name()='Assignment']");
        for (var i=0; i < uiTeamUserNodes.length;  i++)
        {    
            var seqNo = cordys.getNodeText(uiTeamUserNodes[i], ".//*[local-name()='SeqNo']", null);
            var orgUserDN = cordys.getNodeText(uiTeamUserNodes[i], ".//*[local-name()='OrgUserDN']", null);
            var roleDN = cordys.getNodeText(uiTeamUserNodes[i], ".//*[local-name()='RoleDN']", null);
            var assignmentID = cordys.getNodeText(uiTeamUserNodes[i], ".//*[local-name()='AssignmentID']", null);
            var description = cordys.getNodeText(uiTeamUserNodes[i], ".//*[local-name()='Description']", null);
            var assigned = cordys.getNodeText(uiTeamUserNodes[i], ".//*[local-name()='Assigned']", null);
            var option = {value:seqNo, description:description};
            var uiAssignment = {seqNo:seqNo, orgUserDN:orgUserDN, roleDN:roleDN, assignmentID:assignmentID, assigned:assigned};
            if (assigned == 'true')
            {
                listTeamUsers.addOption(option);
                uiAssignment.assigned = true;
            }
            uiAssignments[seqNo] = uiAssignment;
        }  
        assignmentsLoaded = true;
    }
    statusInfo = "";
    setAppInfoBar();      
}

function checkEnableDisable()
{
    if (!assignmentsChanged)
    {
        listTeams.enable();
        btnRefresh.enable();
        btnSave.disable();
        btnUndo.disable();
    }
    else
    {
        listTeams.disable();
        btnRefresh.disable();
        btnSave.enable();
        btnUndo.enable();
    }
}

function btnAddUser_Click(eventObject)
{
    var selectedUsers = listUsers.getSelectedOptions();
    for (var itemIndex in selectedUsers)
    {
        var seqNo = null;
        var orgUserDN = selectedUsers[itemIndex].value;
        var roleDN = listRoles.getSelectedOptions()[0].value;
        var uiAssignment = getUIAssignment(orgUserDN, roleDN);
        var addListOption = false;
        if (uiAssignment != null)
        {
            if (!uiAssignment.assigned)       
            {
                uiAssignment.assigned = true;
                addListOption = true;
            }
            else
            {
                application.notify(selectedUsers[itemIndex].description + " already in the team for role " + listRoles.getSelectedOptions()[0].description);
            }
            seqNo = uiAssignment.seqNo;
        }
        else
        {
            seqNo = uiAssignments.length;
            // add assignment to global array            
            addUIAssignment(seqNo, orgUserDN, roleDN);
            addListOption = true;
        }
        if (addListOption)
        {
            var description = selectedUsers[itemIndex].description;
            var bracketIndex = description.indexOf("(");
            if (bracketIndex > -1)
            {
                description = description.substring(0, bracketIndex-1);
            }
            description = description + "/" + listRoles.getSelectedOptions()[0].description;
            var option = {value:seqNo, description:description};
            listTeamUsers.addOption(option);
            assignmentsChanged = true;  
        }
    }  
    if (assignmentsChanged)
    {
        // undo selection in listUsers
        listUsers.setValue("");
        // put model in edit mode (at same time giving star in title)
        editTeamUsersModel();
        // enable/disable buttons
        checkEnableDisable();
    }
}

function btnRemoveUser_Click(eventObject)
{
    var selectedTeamUsers = listTeamUsers.getSelectedOptions();
    for (var itemIndex in selectedTeamUsers)
    {
        listTeamUsers.removeOption(selectedTeamUsers[itemIndex], false);
        // maintain assignment data - assigned to false
        uiAssignments[selectedTeamUsers[itemIndex].value].assigned = false;
        assignmentsChanged = true;   
    }    
    if (assignmentsChanged)
    {
        editTeamUsersModel();
        checkEnableDisable();
    }      
}

function editTeamUsersModel()
{
    if (UITeamUsersModel.activeTuple != null)
    {
        UITeamUsersModel.edit();
    }
}

function getUIAssignment(orgUserDN, roleDN)
{
    var result = null;
    for (var itemIndex in uiAssignments) 
    {
        if ((uiAssignments[itemIndex].orgUserDN == orgUserDN) && (uiAssignments[itemIndex].roleDN == roleDN))
        {
            result = uiAssignments[itemIndex];
            break;
        }
    }
    return result;
}

function addUIAssignment(seqNo, orgUserDN, roleDN)
{
    var uiAssignment = {seqNo:seqNo, orgUserDN:orgUserDN, roleDN:roleDN, assignmentID:"", assigned:true};
    uiAssignments[seqNo] = uiAssignment;
}

function btnUndo_Click(eventObject)
{
    UITeamUsersModel.undo();
    if (assignmentsChanged)
    {
        assignmentsChanged = false;
    }                
    readData();     
}

function btnRefresh_Click(eventObject)
{
    readTeams();
}

function btnSave_Click(eventObject)
{
    btnSave.disable();
    btnUndo.disable();
    statusInfo = "Saving...";
    setAppInfoBar();      
    application.showBusy(true, "Saving..."); 
    setTimeout("saveData()", 20);    
}

/*
 * Save the user and role assignments
 */
function saveData()
{
    if (assignmentsLoaded && assignmentsChanged )
    {
        UITeamUsersModel.synchronize();
    }  
    statusInfo = "";
    setAppInfoBar();  
    application.showBusy(false);          
}

function UITeamUsersModel_OnSynchronized(eventObject)
{
    if (!UITeamUsersModel.soapFaultOccurred)
    {
        assignmentsChanged = false;
    }    
    checkEnableDisable();    
}

function UITeamUsersModel_BeforeSynchronize(eventObject)
{
    var request = eventObject.updateDocument;
    var tupleNewNode = cordys.selectXMLNode(request, ".//*[local-name()='tuple']/*[local-name()='new']");
    if (tupleNewNode != null)
    {
        var assignmentsNode = cordys.selectXMLNode(tupleNewNode, ".//*[local-name()='Assignments']");
        if (assignmentsNode != null)
        {
            // remove current assignment nodes
            while (assignmentsNode.firstChild) 
            {
                assignmentsNode.removeChild(assignmentsNode.firstChild);
            }
            for (var itemIndex in uiAssignments) 
            {
                var uiAssignment = uiAssignments[itemIndex];
                if (uiAssignment.assigned)
                {
                    var assignment = newAssignment.XMLDocument.documentElement.cloneNode(true);
                    var seqNoNode = cordys.selectXMLNode(assignment, "*[local-name()='SeqNo']")
                    cordys.setTextContent(seqNoNode, uiAssignment.seqNo);
                    var orgUserDNNode = cordys.selectXMLNode(assignment, "*[local-name()='OrgUserDN']")
                    cordys.setTextContent(orgUserDNNode, uiAssignment.orgUserDN);
                    var roleDNNode = cordys.selectXMLNode(assignment, "*[local-name()='RoleDN']")
                    cordys.setTextContent(roleDNNode, uiAssignment.roleDN);
                    var assignedNode = cordys.selectXMLNode(assignment, "*[local-name()='Assigned']")
                    cordys.setTextContent(assignedNode, uiAssignment.assigned.toString());
                    var assignmentIDNode = cordys.selectXMLNode(assignment, "*[local-name()='AssignmentID']")
                    cordys.setTextContent(assignmentIDNode, uiAssignment.assignmentID);   
                    // append to assignments
                    cordys.appendXMLNode(assignment, assignmentsNode);                 
                }
            }
        }
    }
}

function setAppInfoBar()
{
    var info = statusInfo;
    if (info == "")
    {
        info = artifactInfo;
    }
    appInfoBar.setStatus(info);
