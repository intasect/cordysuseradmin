			<xformsscript>
				<![CDATA[/*
 * UI for team assignments
 */
 
// list type
var LT_ROLES = 1;
var LT_TASKS = 2;
var LT_TEAM_ROLES = 3;
var LT_ROLES_FOR_TEAM = 4;
var LT_TEAMS = 5;

var uiAssignments = new Array();        // array of team assignment objects;      

var assignmentsChanged = false;
var assignmentsLoaded = false;

var artifactInfo = "";
var statusInfo = "";
        

function Form_InitDone(eventObject)
{
    setAppInfoBar();
    listUsers.multipleSelect = true;   
    listTeamUsers.multipleSelect = true;
    readTeams();
}     

function readTeams()
{
    // get list of teams and select last selected team or first one
    var lastValue = listTeams.getValue();
    UITeamsModel.reset();
    var allItems = listTeams.getAllOptions();
    if (allItems.length > 0)
    {
        listTeams.setValue(lastValue);
        if (listTeams.getValue() == "")
        {
            // select first one 
            listTeams.setValue(allItems[0].value);
        }
    }
}

function listTeams_Change(eventObject)
{
    artifactInfo = "";
    var selOptions = listTeams.getSelectedOptions();
    if (selOptions.length > 0)
    {
        var bo = selOptions[0].businessObject;
        if (bo != null)
        {
            var longDescNode = cordys.selectXMLNode(bo, ".//*[local-name()='LongDescription']");
            if (longDescNode != null)
            {
                artifactInfo = cordys.getTextContent(longDescNode);
            }
        }
    }
    readRolesForTeam();   
    readData();    
}

function readRolesForTeam()
{
    var lastValue = listRoles.getValue();
    UIRolesForTeamModel.reset();
    var allItems = listRoles.getAllOptions();
    if (allItems.length > 0)
    {
        listRoles.setValue(lastValue);
        if (listRoles.getValue() == "")
        {
            // select first one 
            listRoles.setValue(allItems[0].value);
        }
    } 
}

function listRoles_Change(eventObject)
{
    readUsers();
}

function readUsers()
{
    var lastValue = listUsers.getValue();
    UIOrgUserBasicModel.reset();  
    var allItems = listUsers.getAllOptions();
    if (allItems.length > 0)
    {
        listUsers.setValue(lastValue);
        //if (listUsers.getValue() == "")
        //{
            // select first one 
        //    listRoles.setValue(allItems[0].value);
        //}
    }        
}

/*
 * For the current selected task, read the users, (sub)roles
 */
function readData()
{
    assignmentsLoaded = false; 
    UITeamUsersModel.reset();
}                           

function UITeamsModel_OnRequest(eventObject)
{
