 * Remove team assignmetn
 */
function btnRemoveTeam_Click(eventObject)
{
    var selectedItems = listAssignedTeams.getSelectedOptions();
    for (var itemIndex in selectedItems)
    {
        listUnassignedTeams.addOption(selectedItems[itemIndex],false);
        listAssignedTeams.removeOption(selectedItems[itemIndex], false); 
    }
    clearStatus();            
}

/*
 * Kickoff the synchronizing of ext users, starting at first one
 */
function btnExecute_Click(eventObject)
{
    clearStatus();
    var checkedRows = tblUsers.getCheckedRows();
    if (checkedRows.length > 0)
    {
        cancelHit = false;
        btnExecute.disable();
        btnRefresh.disable();
        btnCancel.enable();
        currentRownr = 0;
        syncNextUser();  
    }
    else
    {
        application.notify("Check one or more users.");    
    }    
}

/*
 * Sync user as per next row
 */
function syncNextUser()
{
    var endOfSync = false;
    if (!cancelHit)
    {
        if (numberOfErrors < MAX_ERRORS)
        {
            currentRownr = getNextCheckedRow(currentRownr);
            if (currentRownr > 0)
            {
                outStatus[currentRownr].setValue("In Process");
                statusCleared = false;
                // user timeout to enable rendering of status column
                setTimeout("syncUser()",200);
            }
            else
            {
                endOfSync = true;
            }
        }
        else
        {
            application.showError("To many errors - sync ext users interrupted.");
            endOfSync = true;
        }
    }
    else
    {
        application.notify("Synchronizing cancelled.");
        endOfSync = true;
    }
    if (endOfSync)
    {
        btnExecute.enable();
        btnRefresh.enable();    
        btnCancel.disable();
    }
}

/*
 * sync ext user
 */
function syncUser()
{
    var tblUserRows = tblUsers.getRows();     
    var bo = tblUserRows[currentRownr-1].businessObject;
    currentExtUserDN = cordys.getNodeText(bo, ".//*[local-name()='ExtUserDN']", null);
    if ((currentExtUserDN != null) && (currentExtUserDN != ""))
    {
        currentExtGroupDN = cordys.getNodeText(bo, ".//*[local-name()='ExtGroupDN']", null);
        SyncExtUserModel.reset();
    }    
    else
    {
        outStatus[currentRownr].setValue("Error");
        syncNextUser();
    }    
}

/*
 * get next row to be synchronized
 */
function getNextCheckedRow(rownr)
{
    var tblUserRows = tblUsers.getRows();     
    var noUsers = tblUserRows.length;
    rownr = rownr + 1;
    var rowFound = false;
    while (!rowFound && (rownr <= noUsers))
    {
        var rowFound = tblUsers.rows[rownr+1].cells[0].firstChild.checked;
        if (!rowFound)
        {
            rownr = rownr + 1;
        }
    }
    if (!rowFound)
    { 
        rownr = -1;
    }
    return rownr;
}

/*
 * Populate SyncExtUser request including selected roles/tasks/teamroles
 */
function SyncExtUserModel_OnRequest(eventObject)
{
    var request = eventObject.request;
    var extUserNode = cordys.selectXMLNode(request, ".//*[local-name()='ExtUserDN']");
    cordys.setTextContent(extUserNode, currentExtUserDN);  
    var extGroupNode = cordys.selectXMLNode(request, ".//*[local-name()='ExtGroupDN']");
    cordys.setTextContent(extGroupNode, currentExtGroupDN);
    // roles 
    var roleAssignmentsNode = cordys.selectXMLNode(request, ".//*[local-name()='RoleAssignments']");
    var templateItemNode = cordys.selectXMLNode(roleAssignmentsNode, "./*[local-name()='UIListItem']");  
    if (templateItemNode != null)
    {
        roleAssignmentsNode.removeChild(templateItemNode);
    }
    var selectedItems = listAssignedRoles.getAllOptions();
    for (var itemIndex in selectedItems)
    {
        var seqNo = selectedItems[itemIndex].value;
        var listItemNode = listRoles[seqNo].listItemNode.cloneNode(true); 
        cordys.appendXMLNode(listItemNode, roleAssignmentsNode);
    }    
    // tasks
    var taskAssignmentsNode = cordys.selectXMLNode(request, ".//*[local-name()='TaskAssignments']");
    var templateItemNode = cordys.selectXMLNode(taskAssignmentsNode, "./*[local-name()='UIListItem']");  
    if (templateItemNode != null)
    {
        taskAssignmentsNode.removeChild(templateItemNode);
    }
    var selectedItems = listAssignedTasks.getAllOptions();
    for (var itemIndex in selectedItems)
    {
        var seqNo = selectedItems[itemIndex].value;
        var listItemNode = listTasks[seqNo].listItemNode.cloneNode(true); 
        cordys.appendXMLNode(listItemNode, taskAssignmentsNode);
    }      
    // teams
    var teamAssignmentsNode = cordys.selectXMLNode(request, ".//*[local-name()='TeamAssignments']");
    var templateItemNode = cordys.selectXMLNode(teamAssignmentsNode, "./*[local-name()='UIListItem']");  
    if (templateItemNode != null)
    {
        teamAssignmentsNode.removeChild(templateItemNode);
    }
    var selectedItems = listAssignedTeams.getAllOptions();
    for (var itemIndex in selectedItems)
    {
        var seqNo = selectedItems[itemIndex].value;
        var listItemNode = listTeams[seqNo].listItemNode.cloneNode(true); 
        cordys.appendXMLNode(listItemNode, teamAssignmentsNode);
    }      
}

function SyncExtUserModel_OnResponse(eventObject)
{
    if (currentRownr > 0)
    {
        if (eventObject.httpStatusCode != 500)
        {
            outStatus[currentRownr].setValue("Done");
        }   
        else
        {
            outStatus[currentRownr].setValue("Error");
        }                 
    }  
    syncNextUser();      
}

function clearStatus()
{
    if (!statusCleared)
    {
        var tblUserRows = tblUsers.getRows();     
        var noUsers = tblUserRows.length;
        for (var i=1; i <= noUsers; i++)     
        {
            outStatus[i].setValue("");
        }
        numberOfErrors = 0;
        statusCleared = true;
    }
}

/**
 * Update the status message 
 */
function setAppInfoBar()
{
    var info = statusInfo;
    if (info == "")
    {
        info = artifactInfo;
    }
    appInfoBar.setStatus(info);
}

function tblUsers_OnSelectRow(eventObject)
{
    var rIndex = eventObject.rowIndex;
    showExtUserDN(rIndex);
}

function showExtUserDN(rIndex)
{
    var extUserDN = "";
    if (rIndex > 0)
    {
        // show ext user DN in status bar
        var tRows = tblUsers.getRows();
        if ((tRows != null) && (tRows.length > 0))
        {
            var bo = tRows[rIndex-1].getBusinessObject();
            if (bo != null)
            {
                var curBO = bo.businessObject;
                if (curBO != null)
                {
                    var nNode = cordys.selectXMLNode(curBO, ".//*[local-name()='ExtUserDN']");
                    if (nNode != null)
                    {
                        extUserDN = cordys.getTextContent(nNode);
                    }
                }
            }
        }
    }
    artifactInfo = extUserDN;
    setAppInfoBar();
}

