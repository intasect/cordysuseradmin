/*
 * Get the ext directory groups as defined in UserSyncConfig
 */
function UIListItemModel_OnRequest(eventObject)
{
    var request = eventObject.request;
    var listTypeNode = cordys.selectXMLNode(request, ".//*[local-name()='ListType']");
    cordys.setTextContent(listTypeNode, LT_EXT_GROUPS); 
    var firstRequestNode = cordys.selectXMLNode(request, ".//*[local-name()='FirstTasksRequest']");
    cordys.setTextContent(firstRequestNode, "false");        
}

function Form_InitDone(eventObject)
{
    setAppInfoBar();
    btnCancel.disable();
    listAssignedRoles.multipleSelect = true;
    listAssignedTasks.multipleSelect = true;
    listAssignedTeams.multipleSelect = true;
    listUnassignedRoles.multipleSelect = true;
    listUnassignedTasks.multipleSelect = true;
    listUnassignedTeams.multipleSelect = true;  
    // populate dropdown with list of ext groups  
    refresh();
}

function refresh()
{
    clearStatus();
    UIListItemModel.reset();
    // populate ext users
    UIExtUserModel.reset();
    readLists();
}

function UIExtUserModel_OnRequest(eventObject)
{
    var groupDN = slctGroup.getValue();
    if ((groupDN != null) && (groupDN != ""))
    {
        var request = eventObject.request;
        var groupDNNode = cordys.selectXMLNode(request, ".//*[local-name()='GroupDN']");
        cordys.setTextContent(groupDNNode, groupDN); 
        var includeSubgroupsNode = cordys.selectXMLNode(request, ".//*[local-name()='IncludeSubgroups']");
        var includeSubgroups = checkSubgroups.getValue();
        cordys.setTextContent(includeSubgroupsNode, includeSubgroups.toString());     
    }
}

function slctGroup_Change(eventObject)
{
    clearStatus();
    UIExtUserModel.reset();    
}

function checkSubgroups_Change(eventObject)
{
    clearStatus();
    UIExtUserModel.reset(); 
}

/*
 * Prepare request for generic list of roles
 */
function UIRoleModel_OnRequest(eventObject)
{
    var request = eventObject.request;
    var listTypeNode = cordys.selectXMLNode(request, ".//*[local-name()='ListType']");
    cordys.setTextContent(listTypeNode, LT_ROLES);   
    var firstTasksRequestNode = cordys.selectXMLNode(request, ".//*[local-name()='FirstTasksRequest']");
    cordys.setTextContent(firstTasksRequestNode, "false");     
}

/*
 * Populate list of roles and global array 
 */
function UIRoleModel_OnResponse(eventObject)
{
    if (eventObject.httpStatusCode != 500)
    {
        var response = eventObject.response;
        var listItemNodes = cordys.selectXMLNodes(response, ".//*[local-name()='old']/*[local-name()='UIListItem']");
        for (var i=0; i < listItemNodes.length; i++)
        {    
            var seqNo = cordys.getNodeText(listItemNodes[i], ".//*[local-name()='SeqNo']", null);
            var description = cordys.getNodeText(listItemNodes[i], ".//*[local-name()='Description']", null);
            var roleDN = cordys.getNodeText(listItemNodes[i], ".//*[local-name()='DN']", null);
            var option = {value:seqNo, description:description};
            var listItem = {seqNo:seqNo,roleDN:roleDN,listItemNode:listItemNodes[i]};
            listUnassignedRoles.addOption(option);
            listRoles[seqNo] = listItem;
        }  
    }      
}

/*
 * Prepare request for generic list of tasks
 */
function UITaskModel_OnRequest(eventObject)
{
    var request = eventObject.request;
    var listTypeNode = cordys.selectXMLNode(request, ".//*[local-name()='ListType']");
    cordys.setTextContent(listTypeNode, LT_TASKS);   
    var firstTasksRequestNode = cordys.selectXMLNode(request, ".//*[local-name()='FirstTasksRequest']");
    cordys.setTextContent(firstTasksRequestNode, getFirstTasksRequest());   
    setFirstTasksRequest(false);  
}

