/*
 * Populate the lists of roles/tasks/teamroles
 */
function readLists()
{
    listUnassignedRoles.removeOptions(listUnassignedRoles.getAllOptions(), false);
    listAssignedRoles.removeOptions(listAssignedRoles.getAllOptions(), false);
    listRoles.length = 0;
    listUnassignedTasks.removeOptions(listUnassignedTasks.getAllOptions(), false);
    listAssignedTasks.removeOptions(listAssignedTasks.getAllOptions(), false);
    listTasks.length = 0;
    listUnassignedTeams.removeOptions(listUnassignedTeams.getAllOptions(), false);
    listAssignedTeams.removeOptions(listAssignedTeams.getAllOptions(), false);
    listTeams.length = 0;   
    UIRoleModel.reset();
    UITaskModel.reset();
    UITeamRoleModel.reset();     
}

/*
 * Move role from unassigned to assigned.
 */
function btnAddRole_Click(eventObject)
{
    var selectedItems = listUnassignedRoles.getSelectedOptions();
    for (var itemIndex in selectedItems)
    {
        listAssignedRoles.addOption(selectedItems[itemIndex],false);
        listUnassignedRoles.removeOption(selectedItems[itemIndex], false); 
    }  
    clearStatus();   
}

